@model List<Bookish.DataAccess.BookInfo>
    <head>
        <link rel="stylesheet" href="~/css/table.css">
    </head>
<div class="text-center">
    <h1 class="display-4">Book List</h1>
    <table>
        <tr>
            <th>ISBN</th>
            <th>Book Name</th>
            <th>Author</th>
            <th>BarCode</th>
            <th>BookInfo</th>
        </tr>
        @foreach (var book in Model)
        {
            <tr>
                <td>@book.ISBN</td>
                <td>@book.BookName</td>
                <td>@book.Author</td>
                <td>@book.BarCode</td>
                <td>
                <button type="button" class="btn btn-primary" value="go away" onclick="location.href='@Url.Action("BookInfo","Home",new {ISBN = book.ISBN})'"/>
            </tr>
                //queery to count number of current available copies
         }
    </table>
</div>